# Environment
ENVIRONMENT=development

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Dantaro Central

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/dantaro_central
DATABASE_TEST_URL=postgresql://postgres:password@localhost:5432/dantaro_central_test

# Redis Cache
REDIS_URL=redis://localhost:6379/0

# API Keys and Authentication
SECRET_KEY=your-super-secret-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# User Server Authentication
USER_SERVER_API_KEY=your-user-server-api-key

# External APIs (Legacy - for backward compatibility)
COINBASE_API_KEY=your-coinbase-api-key
COINBASE_API_SECRET=your-coinbase-api-secret
UPBIT_ACCESS_KEY=your-upbit-access-key
UPBIT_SECRET_KEY=your-upbit-secret-key
BITHUMB_API_KEY=your-bithumb-api-key
BITHUMB_SECRET_KEY=your-bithumb-secret-key
KORBIT_API_KEY=your-korbit-api-key
KORBIT_SECRET_KEY=your-korbit-secret-key
GOPAX_API_KEY=your-gopax-api-key
GOPAX_SECRET_KEY=your-gopax-secret-key
COINONE_ACCESS_TOKEN=your-coinone-access-token

# Modularized Exchange APIs
OKX_API_KEY=your-okx-api-key
OKX_SECRET_KEY=your-okx-secret-key
OKX_PASSPHRASE=your-okx-passphrase
COINONE_API_KEY=your-coinone-api-key
COINONE_SECRET_KEY=your-coinone-secret-key
GATE_API_KEY=your-gate-api-key
GATE_SECRET_KEY=your-gate-secret-key
BYBIT_API_KEY=your-bybit-api-key
BYBIT_SECRET_KEY=your-bybit-secret-key

# AI/ML Configuration
AI_MODEL_PATH=./models
STRATEGY_CACHE_TTL=300

# Monitoring
PROMETHEUS_PORT=8001
LOG_LEVEL=INFO

# Celery
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2
