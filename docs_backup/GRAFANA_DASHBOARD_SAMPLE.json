{
  "dashboard": {
    "id": null,
    "title": "Dantaro Central FastAPI Metrics",
    "panels": [
      {
        "type": "graph",
        "title": "Request Latency (ms)",
        "targets": [
          {
            "expr": "http_request_duration_seconds_bucket{le=\"1\"}",
            "legendFormat": "<=1s"
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "type": "stat",
        "title": "Request Count",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total[5m]))",
            "legendFormat": "Total"
          }
        ],
        "datasource": "Prometheus"
      }
    ],
    "schemaVersion": 30,
    "version": 1
  }
}
